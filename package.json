{
  "name": "create-jakestack",
  "version": "1.0.4",
  "description": "CLI to scaffold a modern full-stack application with Bun, Elysia, React, and PostgreSQL",
  "bin": {
    "create-jakestack": "./dist/index.js"
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --format=cjs --outfile=dist/index.js --banner:js='#!/usr/bin/env node'",
    "dev": "npx tsx src/index.ts"
  },
  "files": [
    "dist",
    "template"
  ],
  "keywords": [
    "cli",
    "scaffold",
    "fullstack",
    "bun",
    "elysia",
    "react",
    "vite",
    "tailwind",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "@types/prompts": "^2.4.9",
    "esbuild": "^0.27.2"
  }
}
