{
  "name": "{{PROJECT_NAME}}",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch server/index.ts",
    "dev:client": "cd client && bun run dev",
    "dev:all": "concurrently \"bun run dev\" \"bun run dev:client\"",
    "build": "cd client && bun run build",
    "start": "bun server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.0",
    "@elysiajs/static": "^1.1.0",
    "@elysiajs/swagger": "^1.1.0",
    "drizzle-orm": "^0.35.0",
    "elysia": "^1.1.6",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@types/bun": "^1.1.6",
    "drizzle-kit": "^0.26.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.39",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.3"
  }
